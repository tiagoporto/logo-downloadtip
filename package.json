  "type": "module",
  "engines": {
    "node": ">=23",
    "npm": "uses pnpm",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
